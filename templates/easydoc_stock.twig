<div style="text-align: right">{{ currencyinfo }}</div>
<table class="items" width="100%" style="font-size: 7pt; border-collapse: collapse; " cellpadding="8">
<thead>
	<tr>
		<th>{{ trans('Ref')}} </th>
		<th>{{ trans('Label')}} </th>
		<th>{{ trans('Qty')}} </th>
		<th>{{ trans('Pmp')}} </th>
	</tr>
</thead>
	{% if lines is iterable %}
		{% for line in lines %}
			<tr>
				<td>{{ line.ref }}</td>
				<td>{{ line.label }}</td>
				<td align="right">{{ price(line.real_qty) }}</td>
				<td align="right">{{ price(line.ppmp) }}</td>
			</tr>
		{% endfor %}
	{% else %}
		{# lines is empty #}
		No lines
	{% endif %}
<tr>
	<td align="left" class="totals" colspan="2">{{ trans('Total') }}</td>
	<td class="totals">{{ totalunit }}</td>
	<td class="totals"></td>
</tr>
</table>
<br />

<br />
{% if object.note_public %}
<table class="items" width="100%" style="font-size: 6pt; border-collapse: collapse; " cellpadding="8">
<tr>
<td>{{ object.note_public }}</td>
</tr>
</table>
<br />
{% endif %}
{% if getDolGlobalString('EASYDOCGENERATOR_ENABLE_DEVELOPPER_MODE') %}
{{ debug }}
{% endif %}
</html>
